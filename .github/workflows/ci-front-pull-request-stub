# .github/workflows/front-end-stub.yml
name: CI Frontend Stub

on:
  pull_request:
    branches:
      - main
      - develop
    paths-ignore:
      - 'front-end/**'

jobs:
  no-front-end-changes:
    runs-on: ubuntu-latest
    steps:
      - run: echo "✅ Nenhuma mudança no front-end. CI não necessário."
